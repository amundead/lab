<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers overrideMode="Allow">
            <!-- PHP handler definition -->
            <add name="PHP" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\php\php-cgi.exe" resourceType="Unspecified" />
        </handlers>

        <defaultDocument>
            <files>
                <add value="index.php" />
            </files>
        </defaultDocument>

        <httpErrors errorMode="Detailed" />
    </system.webServer>
</configuration>
